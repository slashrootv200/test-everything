<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="in.curium.testall"
    xmlns:android="http://schemas.android.com/apk/res/android">
  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
  <application
      android:name=".App"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <!-- activities -->
    <activity android:name=".LauncherActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".activities.FragmentLifeCycleActivity"/>
    <activity android:name=".activities.ParcelableListContainerActivity"/>
    <activity android:name=".activities.ParcelableListContainerReaderActivity"/>
    <activity android:name=".activities.QueriableServiceConnectedActivity"/>
    <activity android:name=".activities.AnimationActivity"/>
    <activity android:name=".activities.ExoMediaActivity"/>
    <activity android:name=".activities.EditTextTestActivity"/>
    <!-- services -->
    <service android:name=".services.QuerieableService"/>
  </application>

</manifest>